    "/api/Auth/RegistrationAsVisitor": {
      "post": {
        "tags": [
          "Auth"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "BornDate",
                  "ConfirmPassword",
                  "Email",
                  "gender",
                  "Name",
                  "Password",
                  "PhoneNumber"
                ],
                "type": "object",
                "properties": {
                  "Name": {
                    "maxLength": 50,
                    "minLength": 2,
                    "type": "string"
                  },
                  "Email": {
                    "type": "string",
                    "format": "email"
                  },
                  "PhoneNumber": {
                    "pattern": "^\\+\\d{8,15}$",
                    "type": "string",
                    "format": "tel"
                  },
                  "Password": {
                    "minLength": 8,
                    "pattern": "^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[@$!%*?&]).{8,}$",
                    "type": "string",
                    "format": "password"
                  },
                  "ConfirmPassword": {
                    "type": "string",
                    "format": "password"
                  },
                  "Address": {
                    "maxLength": 100,
                    "type": "string"
                  },
                  "BornDate": {
                    "type": "string",
                    "format": "date-time"
                  },
                  "gender": {
                    "$ref": "#/components/schemas/Gender"
                  },
                  "ProfilePicture": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "Name": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "PhoneNumber": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "ConfirmPassword": {
                  "style": "form"
                },
                "Address": {
                  "style": "form"
                },
                "BornDate": {
                  "style": "form"
                },
                "gender": {
                  "style": "form"
                },
                "ProfilePicture": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
